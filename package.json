{
  "name": "nuxt-slip-auth",
  "version": "0.1.34",
  "description": "My new Nuxt module",
  "repository": "https://github.com/adrienZ/slip",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "nuxt-slip-auth": "./bin/cli.js"
  },
  "scripts": {
    "postinstall": "nuxt prepare",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "NUXT_SESSION_PASSWORD='7kP@G!hYvZ2#pL1mQx%3vU8c&RwT4oAs9bFnD#' vitest run",
    "test:ci": "pnpm test -- --coverage --reporter=default --reporter=junit && pnpm test:bun",
    "test:watch": "vitest watch",
    "test:bun": "bun test ./src/runtime/database/tests/bun-sqlite.test.bun",
    "test:types": "vue-tsc --noEmit && cd playground && pnpm i && pnpm exec nuxt prepare && vue-tsc --noEmit"
  },
  "dependencies": {
    "@node-rs/argon2": "^1.8.3",
    "@nuxt/kit": "^3.13.1",
    "consola": "^3.2.3",
    "db0": "^0.1.4",
    "drizzle-orm": "^0.33.0",
    "node-ipinfo": "^3.5.3",
    "oslo": "^1.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@libsql/client": "^0.10.0",
    "@nuxt/devtools": "^1.4.1",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.13.1",
    "@nuxt/test-utils": "^3.14.1",
    "@types/bun": "latest",
    "@types/node": "^20.16.5",
    "@vitest/coverage-v8": "^2.0.5",
    "better-sqlite3": "^11.2.1",
    "changelogen": "^0.5.5",
    "citty": "^0.1.6",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "hookable": "^5.5.3",
    "nuxt": "^3.13.1",
    "nuxt-auth-utils": "^0.3.7",
    "typescript": "latest",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  },
  "peerDependencies": {
    "nuxt-auth-utils": "^0.3.2"
  },
  "packageManager": "pnpm@9.9.0"
}